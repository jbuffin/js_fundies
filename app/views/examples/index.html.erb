

<div class="container">
  <h1>Examples</h1>
    <div class="row">
      <div class="span6">

        <table class="table table-bordered">
        <tr>
          <th>Title</th>
        </tr>

      <% @examples.each do |example| %>
        <tr>
          <td><%= example.title %></td>
          <td><%= link_to 'GoTo', example %></td>
          <!--<td><%= link_to 'Edit', edit_example_path(example) %></td>
          <td><%= link_to 'Destroy', example, method: :delete, data: { confirm: 'Are you sure?' } %></td>-->
        </tr>
      <% end %>
      </table>

      <br />

      <%= link_to 'New Example', new_example_path %>
    </div>
  </div>
</div>